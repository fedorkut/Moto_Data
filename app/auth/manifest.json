{
    "manifest_version": 2,
    "name": "Auto Proxy Auth",
    "version": "1.0",
    "permissions": [
      "proxy",
      "tabs",
      "unlimitedStorage",
      "storage",
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "minimum_chrome_version": "22.0.0"
  }

  